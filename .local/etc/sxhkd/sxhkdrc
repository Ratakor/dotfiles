super + shift + p
	librespot-cfg
#super + {m,shift + m}
#	{music,musiccmd}
super + {m,shift + m}
	zpotify play {playlist,album}
XF86Audio{Prev,Next,Play,Stop}
	musiccmd {prev,next,cycle,stop}
super + shift + Left
	musiccmd prev || zpotify prev >/dev/null 2>&1
super + shift + Right
	musiccmd next || zpotify next >/dev/null 2>&1
super + shift + Down
	musiccmd cycle || zpotify pause >/dev/null 2>&1
super + shift + Up
	musiccmd stop
super + {s,a,y,w,e}
	dmenusearch {web,aur,youtube,man,emoji}
super + shift + w
	shutdown-menu
super + u
	dmenuhandler "$(xclip -o)"
super + shift + u
	plumber --sel
