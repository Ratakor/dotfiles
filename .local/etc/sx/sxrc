#!/bin/sh
xrandr --setprovideroutputsource modesetting NVIDIA-0\
	--dpi 96\
	--output HDMI-0 --pos 2560x360\
	--output DP-2 --mode 2560x1440 --rate 144
xwallpaper --output DP-2 --zoom $XDG_DATA_HOME/wallpapers/aiko.jpg
xwallpaper --output HDMI-0 --zoom $XDG_DATA_HOME/wallpapers/DOOM.jpg
setxkbmap fr us

dwmblocks &
xbanish &
gammastep &
picom &

exec dwm #> /tmp/dwm.log 2>&1
